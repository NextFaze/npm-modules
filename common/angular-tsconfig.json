{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "es2016",
      "dom"
    ],
    "moduleResolution": "node",
    "declaration": false,
    "noEmitHelpers": false,
    "noImplicitAny": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "sourceMap": true,
    "pretty": true,
    "noEmit": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "types": ["node"]
  }
}
